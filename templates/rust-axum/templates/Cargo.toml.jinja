[package]
name = "{{ package_name }}"
version = "{{ version }}"
edition = "2024"
description = "Generated Axum API"

[dependencies]
axum = "0.8.7"
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
bytes = "1.10"
jiff = { version = "0.1", features = ["serde"] }
uuid = { version = "1.11", features = ["serde", "v4"] }
rust_decimal = { version = "1.37", features = ["serde"] }

[features]
default = []
{%- for tag in tags %}
{{ tag }} = []
{%- endfor %}
